{
    "name": "mCzolko/atoc-feed-parser",
    "description": "A parser for the ATOC data feed files.",
    "keywords": [
        "ATOC", 
        "Association of Train Operating Companies", 
        "uk rail", 
        "uk train",
        "train feed parser"
    ],
    "license": "MIT",
    "type": "package",
    "support": {
        "issues": "https://github.com/mCzolko/atoc-feed-parser/issues"
    },
    "authors": [{
        "name": "James Smith",
        "homepage": "http://www.linuxsystem.co.uk/",
        "role": "Developer"
    },{
        "name": "Michael Czolko",
        "homepage": "https://github.com/mCzolko",
        "role": "Developer"
    }],
    "require": {
    "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phpunit/php-code-coverage": "~2.0",
        "league/flysystem": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "JCoded\\ATOCFeedParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JCoded\\ATOCFeedParser\\Test\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
